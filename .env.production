# Production API Configuration
REACT_APP_API_URL=https://your-production-domain.com/api/v1
REACT_APP_WS_URL=wss://your-production-domain.com

# Environment
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_SUMUP=true
REACT_APP_ENABLE_HARDWARE=true
REACT_APP_ENABLE_BARCODE_SCANNER=true

# External Services
REACT_APP_SUMUP_APP_ID=your-production-sumup-app-id
REACT_APP_GOOGLE_ANALYTICS_ID=your-production-ga-id

# Build Configuration
GENERATE_SOURCEMAP=false